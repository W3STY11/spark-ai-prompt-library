{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/prompts_index.json", "/assets/*", "/*.png", "/*.jpg", "/*.svg"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
